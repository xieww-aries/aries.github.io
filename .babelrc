{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
            "react-css-modules",
            {
                "generateScopedName": "[name]__[local]___[hash:base64:5]",
                "filetypes": {
                    ".scss": {
                        "syntax": "postcss-scss"
                    }
                },
                "autoResolveMultipleImports": true
            }
        ]
    ]
}
